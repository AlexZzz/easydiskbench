[global]
include ./global

[rw4k_fsync]
bs=4k
rw=randwrite
fsync=1

[rw32k_fsync]
bs=32k
rw=randwrite
fsync=1

[rr4k]
bs=4k
rw=randread

[rr32k]
bs=32k
rw=randread

[rw1m]
bs=1m
rw=randwrite
fsync=1

[rw4m]
bs=4m
rw=randwrite

[rr1m]
bs=1m
rw=randread

[rr4m]
bs=4m
rw=randread

[r1m]
bs=1m
rw=read

[r4m]
bs=4m
rw=read

[r4k_cache_check]
bs=4k
rw=randread
random_distribution=pareto:0.25

[r1m_cache_check]
bs=1m
rw=randread
random_distribution=pareto:0.25
